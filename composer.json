{
  "require": {
    "carlcs/craft-redactorcustomstyles": "3.0.4",
    "craftcms/cms": "3.5.17.1",
    "craftcms/redactor": "2.8.5",
    "nystudio107/craft-imageoptimize": "1.6.20",
    "nystudio107/craft-seomatic": "3.3.28",
    "percipioglobal/craft-colour-swatches": "1.4.1.1",
    "sebastianlenz/linkfield": "1.0.24",
    "spicyweb/craft-neo": "2.8.15.1",
    "verbb/super-table": "2.6.5",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
